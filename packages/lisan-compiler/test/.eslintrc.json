{
  "extends": ["../../../.eslintrc.json", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off",
    "no-template-curly-in-string": "off",
    "@typescript-eslint/ban-ts-ignore": "off",
    "jest/no-try-expect": "off"
  }
}
